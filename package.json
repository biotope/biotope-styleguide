  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "merge": "node demo/runMerge.js",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "vue-cli-service build --watch",
